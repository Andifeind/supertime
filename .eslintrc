{
  extends: "eslint:recommended",
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-console": 1,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    # "no-console": [2, {"allow": ["warn", "error"]}],
    "no-constant-condition": 0
  }
}
